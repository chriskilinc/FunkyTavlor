<Window x:Class="ProjectArtStone.page4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectArtStone"
        mc:Ignorable="d"
        Title="page4" Height="528" Width="668">
    <Grid Background="#FF2B2B2B">
        <Image x:Name="image" HorizontalAlignment="Left" Height="360" Margin="10,10,0,0" VerticalAlignment="Top" Width="640" Source="Assets/640x360 placeholder.png"/>
        <RichTextBox x:Name="richTextBox" HorizontalAlignment="Left" Height="112" Margin="10,375,0,0" VerticalAlignment="Top" Width="360" Background="{x:Null}" Foreground="White">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="button" Content="Redigera" HorizontalAlignment="Left" Margin="550,375,0,0" VerticalAlignment="Top" Width="100" FontFamily="Robotica"/>
        <Button x:Name="button1" Content="Ta bort" HorizontalAlignment="Left" Margin="550,412,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="button2" Content="Tillbaka" HorizontalAlignment="Left" Margin="550,455,0,0" VerticalAlignment="Top" Width="100"/>


    </Grid>
</Window>
