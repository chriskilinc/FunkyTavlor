<Window x:Class="ProjectArtStone.uploadimagejam"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectArtStone"
        mc:Ignorable="d"
        Title="uploadimagejam" Height="300" Width="300">
    <Grid>
        <Image x:Name="imagebox" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="25,38,0,0"/>
        <TextBox x:Name="tbname" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="25,10,0,0"/>
        <Button x:Name="btnsave" Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="25,162,0,0" Click="btnsave_Click"/>
        <Button x:Name="btnnext" Content="Next" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="127,162,0,0" Click="btnnext_Click"/>
        <Button x:Name="Loadimage" Content="LoadImg" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="161,38,0,0" Click="LoadImg_Click"/>



    </Grid>
</Window>
