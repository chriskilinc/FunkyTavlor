<Window x:Class="ProjectArtStoneMain.Upload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectArtStoneMain"
        mc:Ignorable="d"
        Title="Upload" Height="216" Width="384" Background="#FF2B2B2B">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="117" Margin="158,10,0,0" VerticalAlignment="Top" Width="208"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="18" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="93" Margin="60,14,0,0"/>
        <Label x:Name="label" Content="Name:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White"/>
        <Button x:Name="button" Content="Browse" HorizontalAlignment="Left" Margin="10,155,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Save" HorizontalAlignment="Left" Margin="90,155,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
